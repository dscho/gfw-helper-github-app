{
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "indent": ["error", 4],
    "no-trailing-spaces": "error"
  }
}
